    <property name="temp.jar.path.c_war.war" value="${artifacts.temp.dir}/c_war.war"/>
    <property name="temp.jar.path.ScarAdmin_war.war" value="${artifacts.temp.dir}/ScarAdmin_war.war"/>
  </target>
  
  <target name="clean.artifact.scaradmin:war_exploded" description="clean ScarAdmin:war exploded artifact output">
